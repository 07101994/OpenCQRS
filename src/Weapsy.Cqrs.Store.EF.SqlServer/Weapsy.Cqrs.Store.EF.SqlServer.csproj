<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <PackageId>Weapsy.Cqrs.Store.EF.SqlServer</PackageId>
    <Authors>Luca Briguglia</Authors>
    <Company>Weapsy</Company>
    <Product>Weapsy.CQRS</Product>
    <Description>SQL Server database provider for Weapsy.Cqrs event store.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/Weapsy/Weapsy.Cqrs/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright Luca Briguglia</Copyright>
    <PackageProjectUrl>https://github.com/Weapsy/Weapsy.Cqrs</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Weapsy/Weapsy.Cqrs</RepositoryUrl>
    <PackageTags>.net, core, mediator, ddd, cqrs, event sourcing, ef, entity framework, sql, sqlserver, mssql</PackageTags>
    <Version>4.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="2.1.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Weapsy.Cqrs.Store.EF\Weapsy.Cqrs.Store.EF.csproj" />
    <ProjectReference Include="..\Weapsy.Cqrs\Weapsy.Cqrs.csproj" />
  </ItemGroup>

</Project>
