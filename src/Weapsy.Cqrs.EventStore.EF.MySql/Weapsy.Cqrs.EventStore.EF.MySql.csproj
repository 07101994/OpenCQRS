<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <PackageId>Weapsy.Cqrs.EventStore.EF.MySql</PackageId>
    <Authors>Luca Briguglia</Authors>
    <Company>Weapsy</Company>
    <Product>Weapsy.Cqrs</Product>
    <Description>MySQL database provider for Weapsy.Cqrs event store.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/Weapsy/Weapsy.Cqrs/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright Luca Briguglia</Copyright>
    <PackageProjectUrl>https://github.com/Weapsy/Weapsy.Cqrs</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Weapsy/Weapsy.Cqrs</RepositoryUrl>
    <PackageTags>.net, core, mediator, ddd, cqrs, event sourcing, ef, entity framework, mysql</PackageTags>
    <Version>2.1.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MySql.Data.EntityFrameworkCore" Version="6.10.5" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Weapsy.Cqrs.EventStore.EF\Weapsy.Cqrs.EventStore.EF.csproj" />
    <ProjectReference Include="..\Weapsy.Cqrs\Weapsy.Cqrs.csproj" />
  </ItemGroup>

</Project>
